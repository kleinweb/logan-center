{
  "name": "logan-center",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later OR MIT",
  "engines": {
    "node": ">=18",
    "npm": "use-pnpm",
    "pnpm": ">=7",
    "yarn": "use-pnpm"
  },
  "scripts": {
    "codegen:watch": "pnpm run codegen -- --watch",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "pnpm run codegen:watch & && next dev",
    "build": "pnpm run codegen && next build",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "lint": "next lint --ignore-path .gitignore",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "start:wp": "wp-env start",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ladle/react": "^2.4.5",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.2.3",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-codegen/typescript-react-apollo": "^3.3.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.6",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@wordpress/env": "^5.5.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
