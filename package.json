{
  "name": "logan-center",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later OR MIT",
  "engines": {
    "node": ">=18",
    "npm": "use-pnpm",
    "pnpm": ">=7",
    "yarn": "use-pnpm"
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "watch": "concurrently --kill-others-on-fail -n w: 'pnpm:watch:*'",
    "watch:codegen": "pnpm codegen -- --watch",
    "watch:next": "next dev",
    "watch-debug": "concurrently -n w: 'pnpm:watch-debug:*'",
    "watch-debug:next": "NODE_OPTIONS='--inspect' next dev",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "test": "jest",
    "build": "pnpm codegen && pnpm build:next",
    "build:next": "next build",
    "build:storybook": "storybook build",
    "start": "pnpm start:next",
    "start:next": "next start",
    "start:storybook": "storybook dev -p 6006",
    "start:wp": "wp-env start",
    "stop:wp": "wp-env stop"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ladle/react": "^2.4.5",
    "clsx": "^1.2.1",
    "concurrently": "^7.5.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.2.4",
    "next": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@storybook/addon-essentials": "^7.0.0-alpha.47",
    "@storybook/addon-interactions": "^7.0.0-alpha.47",
    "@storybook/addon-links": "^7.0.0-alpha.47",
    "@storybook/nextjs": "^7.0.0-alpha.47",
    "@storybook/react": "^7.0.0-alpha.47",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@wordpress/env": "^5.6.0",
    "autoprefixer": "^10.4.13",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.1",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "storybook": "^7.0.0-alpha.47",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  }
}
