{
  "name": "logan-center-frontend",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">=18",
    "yarn": ">=3 <4",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "next build",
    "codegen:watch": "yarn codegen -- --watch",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "lint": "next lint --ignore-path .gitignore",
    "prebuild": "yarn codegen",
    "predev": "yarn codegen:watch &",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "ioredis": "^4.28.5",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-storybook": "^0.6.6",
    "jest": "^27.5.1",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.8.4",
    "util": "^0.12.5"
  },
  "peerDependencies": {
    "@types/node": "*",
    "graphql-tag": "*",
    "ts-node": "*"
  }
}
