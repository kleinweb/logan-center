{
  "name": "logan-center-frontend",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">=18",
    "yarn": ">=3 <4",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "next build",
    "codegen:watch": "yarn codegen -- --watch",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "lint": "next lint --ignore-path .gitignore",
    "prebuild": "yarn codegen",
    "predev": "yarn codegen:watch &",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-codegen/typescript-react-apollo": "^3.3.4",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "ioredis": "^5.2.3",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^29.1.2",
    "@types/node": "18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-jest": "^27.1.2",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "postcss": "^8.4.18",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "graphql-tag": "*",
    "util": "*"
  }
}
