  "private": true,
  "author": {
    "name": "Klein College of Media and Communication",
    "email": "kleinweb@temple.edu",
    "url": "https://github.com/kleinweb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kleinweb/logan-center"
  },
  "bugs": {
    "url": "https://github.com/kleinweb/logan-center/issues",
    "email": "kleinweb+bugs@temple.edu"
  },
  "homepage": "https://templelogancenter.org",
  "engines": {
    "node": ">=16",
    "npm": "use-yarn-instead"
  },
  "scripts": {
    "prepare": "husky install",
    "codegen": "turbo run codegen",
    "dev": "concurrently 'yarn run codegen:watch' 'dotenv -- turbo run dev --no-cache --continue'",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "build": "yarn run codegen && turbo run build",
    "copy": "turbo run copy"
  },
  "workspaces": [
    "apps/nextjs",
    "packages/eslint-config-custom",
    "packages/tailwind-config",
    "packages/tsconfig",
    "packages/tu-assets",
    "packages/ui"
  ],
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/fragment-matcher": "3.3.3",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "cross-fetch": "^3.1.5",
    "dotenv": "16.0.3",
    "dotenv-cli": "^7.0.0",
    "graphql": "^16.6.0",
    "graphql-config": "^4.4.1",
    "graphql-tag": "^2.12.6",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/prettier": "^2.7.2",
    "commitlint": "^17.4.2",
    "concurrently": "^7.6.0",
    "dotenv-vault": "^1.18.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "just-install": "^1.0.11",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "turbo": "^1.7.4",
    "typescript": "^4.9.5"
  }
}
