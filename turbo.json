{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"],
      "inputs": ["**/*.tsx", "**/*.ts", "test/**/*.ts"]
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
