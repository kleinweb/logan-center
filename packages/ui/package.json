{
  "name": "ui",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css",
    "./assets/": "./assets/"
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.ts --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.2.2",
    "eslint": "^8.0.0",
    "eslint-config-custom": "workspace:*",
    "rimraf": "^3.0.2",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.1.5",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
