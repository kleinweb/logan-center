{
  "name": "ui",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "tsup src/index.ts --format esm,cjs --dts --external react",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently 'npm:dev:*'",
    "dev:js": "npm run build:js -- --watch",
    "dev:css": "npm run build:css -- --watch",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "lucide-react": "^0.100.0",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shared-assets": "*"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-config-custom": "*",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "rimraf": "^3.0.2",
    "tailwind-config": "*",
    "tailwindcss": "^3.2.3",
    "tsconfig": "*",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4"
  }
}
