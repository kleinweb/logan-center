{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "strict": false,
    "noImplicitAny": false,
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
