{
  "name": "@kleinweb/logan-center-nextjs",
  "version": "0.3.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts --require dotenv/config",
    "codegen:watch": "yarn run codegen -- --watch",
    "dev": "NODE_OPTIONS='-r dotenv-vault-core/config' next dev",
    "dev:debug": "NODE_OPTIONS='--inspect -r dotenv-vault-core/config' next dev",
    "build": "NODE_OPTIONS='-r dotenv-vault-core/config' next build",
    "start": "NODE_OPTIONS='-r dotenv-vault-core/config' next start",
    "lint": "tsc --noEmit && next lint",
    "copy": "concurrently 'yarn:copy:*'",
    "copy:tu-assets": "rimraf public/assets/shared && cp -R ../../packages/tu-assets ./public/assets/shared",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/fragment-matcher": "3.3.3",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@kleinweb/logan-center__ui": "workspace:*",
    "@kleinweb/tu-assets": "workspace:*",
    "@wordpress/i18n": "4.26.1",
    "clsx": "^1.2.1",
    "dotenv-vault-core": "^0.7.0",
    "graphql": "^16.6.0",
    "lucide-react": "^0.112.0",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.11.2",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@kleinweb/eslint-config-custom": "workspace:*",
    "@kleinweb/logan-center__tailwind-config": "workspace:*",
    "@kleinweb/tsconfig": "workspace:*",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "graphql-tag": "^2.12.6",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
