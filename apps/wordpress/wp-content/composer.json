{
  "name": "kleinweb/logan-center",
  "description": "Logan Center for Urban Investigative Reporting",
  "version": "1.0.0",
  "homepage": "https://github.com/kleinweb/logan-center",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Klein College of Media and Communication",
      "email": "kleinweb@temple.edu"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org/"
    },
    {
      "type": "path",
      "url": "../../../packages/logan-center-wp-plugin"
    },
    {
      "type": "path",
      "url": "../../../packages/logan-center-wp-theme"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "kleinweb/logan-center-wp-plugin": "dev-main",
    "nextjswp/logan-center-wp-theme": "dev-main",
    "wp-graphql/wp-graphql-acf": "^0.3",
    "wpackagist-plugin/add-wpgraphql-seo": "^4.0",
    "wpackagist-plugin/advanced-custom-fields": "^6.0",
    "wpackagist-plugin/custom-post-type-ui": "^1.0",
    "wpackagist-plugin/wordpress-seo": "^19.0",
    "wpackagist-plugin/wp-graphql": "^1.0"
  }
}
